var module = this.module || {};
(function(name,fn){
  if(!module['exports']) module.exports = {};
  module.exports[name] = fn(module.exports);
})('Core',function(global){

	global.ToolStack.load(['Callback','Promise'],function(ts){

		var Core = {};

		ts.ns('Core.Core',Core,global);
	});
});


var module = module || {};
(function(name,fn){
  if(!module['exports']) module.exports = {};
  module.exports[name] = fn;
})('Core',function(global,callback){

	global.ToolStack.load(['Class','ToolChain'],function(ts){

		var Core = {};

		var ns = ts.ns('Core.Core',Core,global);
		if(callback) callback(global);

		if(typeof module !== 'undefined' && typeof require !== 'undefined'){
			module.exports = ns;
		} 
	});
});